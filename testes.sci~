clc
clear

function percentage = perRoulette(vec, ind)
    soma = 0;
    
    for i = 1:3
        soma = 1/vec(i) + soma; // [0.1 0.2 0.3]
    end
    
    percentage = 100 * ((1/ind) / soma);
endfunction

vec = [1 2 3];

disp(perRoulette(vec, vec(1)));

